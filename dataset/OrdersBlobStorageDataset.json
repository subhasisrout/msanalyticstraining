{
	"name": "OrdersBlobStorageDataset",
	"properties": {
		"linkedServiceName": {
			"referenceName": "OrdersBlobStorageConnection",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"containerName": {
				"type": "string",
				"defaultValue": "orders"
			},
			"folder": {
				"type": "string",
				"defaultValue": "/"
			},
			"fileName": {
				"type": "string",
				"defaultValue": "orders-1.csv"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().fileName",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().folder",
					"type": "Expression"
				},
				"container": {
					"value": "@dataset().containerName",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}